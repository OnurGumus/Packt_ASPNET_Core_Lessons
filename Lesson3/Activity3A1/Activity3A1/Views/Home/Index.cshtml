<html>
<head>
    <title> Prime numbers</title>
</head>
<body>
    @functions{
        public bool IsPrime(int n)
        {
            if (n <= 1) return false;
            if (n <= 3) return true;
            if (n % 2 == 0 || n % 3 == 0) return false;

            for (var i = 5; i * i <= n; i = i + 6)
                if (n % i == 0 || n % (i + 2) == 0)
                    return false;
            return true;

        }
    }
    <ul>
        @{
            for (var i = 1; i < 100; i++)
            {
                if (IsPrime(i))
                {
                    <li>@i</li>
                }
            }
        }
    </ul>


</body>
</html>
